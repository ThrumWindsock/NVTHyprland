window {
  background-color: @bg;
  border-color: @border-color2;
}

prompt
{
  background-color: transparent;
  spacing: 10px;
  color: @text-color;
  padding: 10 0 10 0;
}
mainbox {
  children: [inputbar, listview];
}
listview {
  border: 2px;
  border-color: @border-color1;
  spacing: 2px;
  scrollbar: true;
  padding: 5px 0px 5px 5px;
}

scrollbar {
  background-color: @text-color;
  border-color: @border-color2;
  handle-color: @scrollbar-color;
  width: 10px;
}

entry {
  background-color: @bg;
  padding: 10 0 10 0;
  text-color: @text-color;
}
element {
  background-color: transparent;
  border-color: @scrollbar-color;
  children: [element-icon, element-text]; 
}
element normal.normal {
    background-color: @bg;
    text-color: @text-color;
    border: 0 0 3 10;
    border-color: @ltorangemenu;
}
element normal.urgent {
    background-color: @bg;
    text-color: @text-color;
    border: 0 0 3 10;
    border-color: @ltorangemenu;
}
element normal.active {
    background-color: @bg;
    text-color: @ltorangemenu;
    border: 0 0 3 10;
    border-color: @drkred;
}
element selected.normal {
    background-color: @drkred;
    text-color: @ltorangemenu;
    border: 0 0 3 10;
    border-color: @drkred;
}
element selected.urgent {
    background-color: @drkred;
    text-color: @ltorangemenu;
    border: 0 0 3 10;
    border-color: @text-color;
}
element selected.active {
    background-color: @drkred;
    text-color: @ltorangemenu;
    border: 0 0 3 10;
    border-color: @text-color;
}
element alternate.normal {
    background-color: @bg;
    text-color: @text-color;
    border: 0 0 3 10;
    border-color: @ltblumenu;
}
element alternate.urgent {
    background-color: var(alternate-urgent-background);
    text-color:       var(alternate-urgent-foreground);
}
element alternate.active {
    background-color: @bg;
    text-color: @ltblumenu;
    border: 0 0 3 10;
    border-color: @border-color2;
}
element selected {
  background-color: @drkred;
}
element-text {
  background-color: @bg;
  text-color: @text-color;
  border: 0px;
  padding: 0 0 10 0;
}
element-icon {
  size: 32 px;
  background-color: @bg;
  padding: 0 6 0 0;
  alignment: vertical;
}
